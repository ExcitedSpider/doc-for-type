<%
  const getPropsByType = (typeDef)=>{
    if(typeDef.type==='object'){
      return {...getPropsByType(typeDef.properties)}
    }else if(typeDef.type ==='array'){
      return {type: 'array', items: {...getPropsByType(typeDef.items)}}
    }else if(typeof typeDef === 'object'){
      const map = {}
      Object.keys(typeDef).forEach(key => {
        Object.assign(map, {...getPropsByType(typeDef[key])})
      });
      return typeDef
    }else{
      return typeDef
    }
  }

  const generateMap = (typeDesc)=>{
    const map = {}
    if(typeDesc.anyOf){
      typeDesc.anyOf.forEach((subUnionType, index)=>{
        Object.assign(map, getPropsByType(subUnionType))
      })
    }else {
      Object.assign(map, getPropsByType(typeDesc))
    }

    return map
  }

  const mapToTupleList = (map)=>{
    const tupleList = []
    Object.keys(map).forEach(key=>{
      tupleList.push([key, map[key]])
    })
    return tupleList
  }

  const list = mapToTupleList(generateMap(typeDesc))
%> 


