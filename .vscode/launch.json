  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run doc-by-type",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/lib/cli.js",
      "args": ["--path", "./src/compiler/type.ts", "--type-name", "TypeDocData"],
      "outputCapture": "console",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "particular test case",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/lib/cli.js",
      "args": ["--path", "./test/index-type/main.ts", "--type-name", "MyObject"],
      "outputCapture": "console",
      "sourceMaps": true
    }
  ]
}